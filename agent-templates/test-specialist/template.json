{
  "name": "test-specialist", 
  "description": "Testing specialist agent for comprehensive test creation, analysis, and quality assurance",
  "version": "1.1.0",
  "author": "Vibe Ensemble Team",
  "variables": [
    {
      "name": "project_name",
      "description": "Name of the project to test",
      "variable_type": "String",
      "default_value": null,
      "required": true,
      "constraints": {
        "min": 1,
        "max": 100,
        "pattern": "^[a-zA-Z0-9][a-zA-Z0-9_-]*$",
        "allowed_values": null
      }
    },
    {
      "name": "primary_language",
      "description": "Primary programming language for testing",
      "variable_type": "String", 
      "default_value": "rust",
      "required": false,
      "constraints": {
        "min": null,
        "max": null,
        "pattern": null,
        "allowed_values": ["rust", "python", "javascript", "typescript", "java", "go"]
      }
    },
    {
      "name": "test_coverage_target",
      "description": "Target code coverage percentage",
      "variable_type": "Number",
      "default_value": 80,
      "required": false,
      "constraints": {
        "min": 50,
        "max": 100,
        "pattern": null,
        "allowed_values": null
      }
    },
    {
      "name": "test_types",
      "description": "Types of tests to focus on (comma-separated)",
      "variable_type": "String",
      "default_value": "unit,integration,end-to-end",
      "required": false,
      "constraints": null
    },
    {
      "name": "testing_framework",
      "description": "Preferred testing framework",
      "variable_type": "String",
      "default_value": "default",
      "required": false,
      "constraints": {
        "min": null,
        "max": null, 
        "pattern": null,
        "allowed_values": ["default", "jest", "pytest", "junit", "mocha", "cypress", "selenium"]
      }
    },
    {
      "name": "enable_property_testing",
      "description": "Whether to include property-based testing",
      "variable_type": "Boolean",
      "default_value": false,
      "required": false,
      "constraints": null
    },
    {
      "name": "performance_testing",
      "description": "Whether to include performance/load testing",
      "variable_type": "Boolean",
      "default_value": false, 
      "required": false,
      "constraints": null
    }
  ],
  "capabilities": [
    "unit-testing",
    "integration-testing", 
    "end-to-end-testing",
    "test-automation",
    "coverage-analysis",
    "performance-testing",
    "property-testing",
    "test-documentation",
    "ci-cd-integration"
  ],
  "tool_permissions": {
    "allowed_tools": [
      "Read",
      "Write", 
      "Edit",
      "Glob",
      "Grep",
      "Bash"
    ],
    "denied_tools": [],
    "file_access": {
      "read": true,
      "write": true,
      "allowed_paths": [],
      "denied_paths": [
        "/etc/**",
        "/usr/**",
        "/var/**",
        "~/.ssh/**"
      ]
    },
    "network_access": {
      "enabled": true,
      "allowed_domains": [
        "github.com",
        "gitlab.com",
        "crates.io",
        "npmjs.com",
        "pypi.org"
      ],
      "denied_domains": []
    },
    "process_access": {
      "enabled": true,
      "allowed_commands": [
        "cargo",
        "npm",
        "yarn",
        "python",
        "pytest",
        "jest", 
        "go",
        "mvn",
        "gradle"
      ],
      "denied_commands": [
        "rm",
        "chmod",
        "sudo",
        "su"
      ]
    }
  },
  "tags": [
    "testing",
    "quality-assurance", 
    "automation",
    "coverage"
  ],
  "min_claude_version": "1.0.0",
  "created_at": "2024-08-20T16:30:00Z",
  "updated_at": "2024-08-21T11:00:00Z"
}