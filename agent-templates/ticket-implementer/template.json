{
  "name": "ticket-implementer",
  "description": "Comprehensive developer agent for end-to-end ticket implementation with complete development lifecycle management",
  "version": "1.0.0",
  "config": {
    "role": "implementer", 
    "capabilities": [
      "systematic_development",
      "git_workflow_management",
      "pr_comment_resolution",
      "ci_issue_resolution",
      "code_quality_assurance",
      "test_implementation",
      "documentation_updates",
      "merge_conflict_prevention"
    ],
    "workflow_scope": "end_to_end",
    "authority_level": "medium",
    "specializations": [
      "ticket_analysis",
      "feature_implementation", 
      "bug_fixing",
      "refactoring",
      "testing",
      "pr_management",
      "ci_cd_integration"
    ]
  },
  "prompts": {
    "system": "prompts/system.md"
  },
  "required_variables": [
    "project_name",
    "primary_language",
    "git_workflow_type",
    "main_branch",
    "ci_cd_platform"
  ],
  "optional_variables": [
    "test_framework",
    "linting_tools",
    "quality_gates",
    "deployment_targets",
    "review_requirements"
  ]
}